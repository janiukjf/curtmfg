@{
    List<string> years = ViewBag.years;
}
<span class="clear"></span>
<div id="hitchlookup">
    <span id="closeLookup">X</span>
    <p>Quick Hitch Lookup</p>
    <form method="get" id="hitchLookupForm" name="hitchLookup_compressed" action="@Url.Content("~/HitchLookup/find")">
        <div>
            <!-- Select Mount -->
            <select name="mount" id="select_mount" required>
                <option value="">Select Mount</option>
                <option>Rear Mount</option>
                <option>Front Mount</option>
            </select>
            <img src="/Content/img/ajax-loader-small.gif" alt="loading..." style="display:none;"/>
        </div>
        <div>
            <!-- Select Year -->
            <select disabled="disabled" name="year" id="select_year" required>
                <option value="">Select Year</option>
                @*foreach (string year in years) {
                    <option value="@year">@year</option>
                }*@
            </select>
            <img src="/Content/img/ajax-loader-small.gif" alt="loading..." style="display:none;"/>
        </div>
        <div>
            <!-- Select Make -->
            <select disabled="disabled" name="make" id="select_make" required>
                <option value="">Select Make</option>
            </select>
            <img src="/Content/img/ajax-loader-small.gif" alt="loading..." style="display:none;"/>
        </div>
        <div>
            <!-- Select Model -->
            <select disabled="disabled" name="model" id="select_model" required>
                <option value="">Select Model</option>
            </select>
            <img src="/Content/img/ajax-loader-small.gif" alt="loading..." style="display:none;"/>
        </div>
        <div>
            <!-- Select Style -->
            <select disabled="disabled" name="style" id="select_style" required>
                <option value="">Select Style</option>
            </select>
            <img src="/Content/img/ajax-loader-small.gif" alt="loading..." style="display:none;"/>
            <span class="clear"></span>
        </div>
        <div>
            <p id="lookupError" style="display:none;"></p>
            <input type="submit" class="btnSubmit button" id="btnSubmit" value="Search" />
        </div>
    </form>
</div>