@using curtmfg.Models;
@{
    ViewBag.Title = "Hitch Lookup - Select a Year";
    ViewBag.css = new List<string> { Url.Content("~/Content/css/hitchLookup.css") };
    Layout = "~/Views/Shared/_InternalLayout.cshtml";
    string mount = ViewBag.mount;
    List<string> years = ViewBag.years;
}

<h4 id="vehicle" class="hasvehicle"><span><span class="yourvehicle"></span> &nbsp;</span></h4>
<h3>Select Your Vehicle's Year</h3>
<ul class="lookup">
    @foreach (string year in years) {
        <li><a href="/HitchLookup/Mount/@mount/Year/@year">@year</a></li>
    }
</ul>